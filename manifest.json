{
	"manifest_version": 2,
	"name": "BpmonlinePuhuhulizerMod",
	"version": "1.22",
	"permissions": ["tabs"],
	"permissions": ["storage"],
	"content_scripts":[
		{
			"matches": [
				"https://ffm.ukrtelecom.net/*"
			],
			"js": [
				"content.js"
			]
		}
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "lasthope.png"
	},
	"icons": {
		"16" : "lasthope.png",
		"48" : "lasthope.png",
		"128" : "lasthope.png"
	}
}